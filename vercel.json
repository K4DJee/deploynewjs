{
    "version": 2,
    "builds": [
      {
        "src": "server.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/register",
        "dest": "/register" 
      },
      {
        "src": "/login",
        "dest": "/login" 
      },
      {
        "src": "/admin-panel",
        "dest": "/admin-panel" 
      },
      {
        "src": "/(.*)",
        "dest": "/" 
      }
    ]
  }
